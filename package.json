    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-junit": "^10.0.0",
    "jest-plugin-fs": "^2.9.0",
    "jsdoc": "^3.6.3",
    "jsdoc-to-markdown": "^5.0.0",
    "openapi-schema-validator": "^3.0.3",
    "tsd-jsdoc": "^2.4.0",
    "typescript": "^4.5.4"
  }
