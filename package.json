    "@adobe/aio-lib-core-logging": "1.1.0",
    "cross-fetch": "^3.0.4",
    "fetch-mock": "^9.3.1",
    "swagger-client": "3.10.2"
  },
  "plugins": [
    "istanbul "
  ],
  "devDependencies": {
    "@adobe/eslint-config-aio-lib-config": "^1.1.0",
    "@types/node-fetch": "^2.5.4",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-runtime": "^6.26.0",
    "codecov": "^3.5.0",
    "dotenv": "^8.1.0",
    "eol": "^0.9.1",
    "eslint": "^6.2.2",
